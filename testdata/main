function select_option(form) 
       {
        var i=0;
        var j=0;
        var ans;


	       if (form.elements[1].value=="") 
			      {
         	   alert("Please select an option.");
		         return; 
            } form.elements[3].value="Enter Keywords or Course Code"; return true;
      } 
     
</script>
<INPUT style="font-size: 8pt" TYPE="button" VALUE="Load Class Schedule" OnClick="if (select_option(this.form)) {this.form.action='AUS_SCHEDULE.main_display1';this.form.boption.value='CLoad';submit()}">
<BR>
<BR>
<script>
function search_text(form) 
       {
        var i=0;
        var j=0;
        var ans;


	       if (form.elements[3].value=="" ||form.elements[3].value=="Enter Keywords or Course Code") 
			      {
         	   alert("Please enter keywords.");form.elements[3].value="Enter Keywords or Course Code";
		         return; 
            } form.elements[1].value=""; return true;
      } 
     
</script>
